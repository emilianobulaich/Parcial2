#ifndef employee_H_INCLUDED
#define employee_H_INCLUDED
typedef struct
{
    int id;
    char nombre[128];
    int horasTrabajadas;
    int sueldo;
}Employee;

typedef struct{
	int id;
	char titulo[128];
	char autor[128];
	int idEditorial;
}eLibro;

eLibro* employee_new();
eLibro* employee_newParametros(char* idStr,char* nombreStr,char* horasTrabajadasStr, char* sueldo);
void employee_delete(eLibro* this);

int employee_setId(eLibroc* this,int id);
int employee_getId(eLibro* this,int* id);

int employee_setNombre(eLibro* this,char* nombre);
int employee_getNombre(eLibro* this,char* nombre);

int employee_setHorasTrabajadas(eLibro* this,int horasTrabajadas);
int employee_getHorasTrabajadas(eLibro* this,int* horasTrabajadas);

int employee_setSueldo(eLibro* this,int sueldo);
int employee_getSueldo(eLibro* this,int* sueldo);

int employee_orderById(void* firstId, void* secondId);
int employee_orderByName(void* firstName, void* secondName);
int employee_orderBySalary(void* firstSalary, void* secondSalary);
int employee_orderByHours(void* firstHours, void* secondHours);

#endif // employee_H_INCLUDED
