#ifndef EDITORIAL_H_
#define EDITORIAL_H_

typedef struct{
	int idEditorial;
	char nombre[128];
}eEditorial;

eEditorial* book_new();
eEditorial* book_newParametros(char* idStr, char* tituloStr, char* autorStr, char* precioStr, char* idEditorialStr);
void book_delete(eEditorial* this);

int book_setId(eEditorial* this,int id);
int book_getId(eEditorial* this,int* id);

int book_setTitulo(eEditorial* this,char* titulo);
int book_getTitulo(eEditorial* this,char* titulo);


#endif /* EDITORIAL_H_ */
